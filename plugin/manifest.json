{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "QuizletBot",
  "description": "Automatically solves Quizlet Scatter and Gravity. Created by Jackson Tullis - 2016 - Apache 2.0 License",
  "version": "2.1.1",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons":{
    "128": "icon.png"
  },
  "permissions": [
    "activeTab","tabs","<all_urls>"
  ]
}
